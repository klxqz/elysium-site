        </file>
        <file path="blue.css">
            <description locale="en_US">CSS styles "blue" theme</description>
            <description locale="ru_RU">CSS стили темы "Синий"</description>
        </file>
        <file path="green.css">
            <description locale="en_US">CSS styles "green" theme</description>
            <description locale="ru_RU">CSS стили темы "Зеленый"</description>
        </file>
        <file path="orange.css">
            <description locale="en_US">CSS styles "orange" theme</description>
            <description locale="ru_RU">CSS стили темы "Оранжевый"</description>
        </file>
        <file path="purple.css">
            <description locale="en_US">CSS styles "purple" theme</description>
            <description locale="ru_RU">CSS стили темы "Фиолетовый"</description>
        </file>
        <file path="red.css">
            <description locale="en_US">CSS styles "red" theme</description>
            <description locale="ru_RU">CSS стили темы "Красный"</description>
        </file>
    </files>
    <settings>
        <setting var="logo" control_type="image">
            <value>img/logo.png</value>
            <filename>img/logo.*</filename>
            <name locale="en_US">Company Logo</name>
            <name locale="ru_RU">Логотип компании</name>
        </setting>
        <setting var="phone" control_type="text">
            <value>+7-999-999-99-99</value>
            <name locale="en_US">Phone</name>
            <name locale="ru_RU">Телефон. Если у Вас установлено приложение "Магазин" вместо этого номера будет выводиться номер из настроек магазина.</name>
        </setting>
        

        
        
    </settings>
